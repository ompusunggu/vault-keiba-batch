{
  "keiba-batch": {
    "database": {
      "username": "keiba-batch",
      "password": "keiba-batch",
      "url": "jdbc:postgresql://localhost:5432/keiba_db",
      "driver": "org.postgresql.Driver"
    },
    "api": {
      "apiKey": "your-api-key-here",
      "apiSecret": "your-api-secret-here",
      "endpoint": "https://api.example.com"
    },
    "config": {
      "environment": "development",
      "logLevel": "DEBUG",
      "batchSize": "100",
      "cronExpression": "0 0 * * * ?"
    },
    "services": {
      "consul": {
        "token": "consul-token-here",
        "host": "localhost",
        "port": "8500"
      },
      "redis": {
        "password": "redis-password-here",
        "host": "localhost",
        "port": "6379"
      },
      "rabbitmq": {
        "username": "keiba_rmq",
        "password": "keiba_rmq_pass",
        "host": "localhost",
        "port": "5672"
      }
    }
  }
}
